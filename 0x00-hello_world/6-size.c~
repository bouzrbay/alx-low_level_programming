#include <stdio.h>
/**
 *main - prints the size of various types based on
 * the computer it is compiled and run on..
 *Rturn: Always 0.
 **/
int main(void)
{
printf("Size of a char: %zu byte(s)\n", sizeof(char));
printf("Size of an int: %zu byte(s)\n", sizeof(int));
printf("Size of a long int: %zu byte(s)\n", sizeos(long int));
printf("Size of long lond int: %zu byte(s)\n", sizeof(long long int));
printf("Size of a float: %zu byte(s)\n" sizeof(loat));
return (0);
}
